{
  "name": "z-vuex-typed",
  "type": "module",
  "version": "0.2.9",
  "packageManager": "pnpm@7.33.6",
  "description": "",
  "author": "zcf0508 <zcf0508@live.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/zcf0508/vuex-typed#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zcf0508/vuex-typed.git"
  },
  "bugs": "https://github.com/zcf0508/vuex-typed/issues",
  "keywords": [
    "vuex",
    "typescript"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">= 14.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "bumpp",
    "start": "esno src/index.ts",
    "test": "vitest ./test/index3",
    "test:once": "vitest run ./test/index3 & tsc ./test/index3.test.ts --noEmit",
    "typecheck": "tsc --noEmit",
    "play": "npm -C playground/vue2 run dev"
  },
  "peerDependencies": {
    "vue": ">=2.0.0 || >=3.0.0",
    "vuex": ">=3.0.0 || >=4.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@antfu/utils": "^0.7.5",
    "@types/node": "^20.4.5",
    "bumpp": "^9.1.1",
    "eslint": "^8.45.0",
    "esno": "^0.17.0",
    "happy-dom": "^10.11.2",
    "pnpm": "^8.6.10",
    "rimraf": "^5.0.1",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^4.8.4",
    "unbuild": "^1.2.1",
    "vite": "^4.4.7",
    "vitest": "^0.34.6",
    "vue": "2.7.14",
    "vuex": "3.6.2"
  },
  "pnpm": {
    "patchedDependencies": {
      "vuex@3.6.2": "patches/vuex@3.6.2.patch"
    }
  }
}
